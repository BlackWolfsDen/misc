-- idea by 125125
-- code by slp13at420
local GM_Gear = {}
local GM_Gear = {2586,11508,12064,17,192} -- Robe, Slippers, Hood, Martins Fury, Martins Staff)

function GM_gearme(event, player, message, type, language)
	if(message == "#gmgearme")then
		if(player:GetGMRank()>=1)then
			for _, v in ipairs(GM_Gear) do
				player:AddItem(v, 1)
			end
		end
	end
end

RegisterPlayerEvent(18, GM_gearme)
