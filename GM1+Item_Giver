-- code by slp13at420
local VIP_Gear = {}
local VIP_Gear = {2586,11508,12064,17,192} -- Robe, Slippers, Hood, Martins Fury, Martins Staff)

function VIP_gearme(event, player, message, type, language)
	if(message == "#gmgearme")then
		if(player:GetGMRank()>=1)then
			for _, v in ipairs(VIP_Gear) do
				player:AddItem(v, 1)
			end
		end
	end
end

RegisterPlayerEvent(18, VIP_gearme)
